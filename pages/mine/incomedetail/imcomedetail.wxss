/*container*/
.container{
	background-color: #FAFAFA;
}

/*top*/
.top{
	width: 100%;
	height: 730rpx;
	overflow: hidden;
}
.border{
	width: 100%;
	height: 560rpx;
	position: relative;
}
.imageview{
	width: 100%;
	height: 560rpx;
	position: absolute;
	left: 0;
	top: -100rpx;
}
.bgimage{
	width: 100%;
	height: 640rpx;
}
.textview{
	width: 100%;
	height: 640rpx;
	color: #FFFFFF;
	text-align: center;
	position: absolute;
	left: 0;
	top: 0;
}
.dateview{
	align-items: center;
	margin-top: 26rpx;
	margin-bottom: 86rpx;
}
.dateview text{
	font-size: 24rpx;
	/* margin-left: 40%; */
}
.textview image{
	width: 24rpx;
	height: 12rpx;
	margin-left: 10rpx;
}
.income{
	font-size: 48rpx;
	font-weight: 500;
	font-family:PingFangSC-Medium;
	margin-bottom: 31rpx;
}
.income2{
	font-size: 30rpx;
	font-weight: 400;
	font-family:PingFangSC-Regular;
	margin-bottom: 20%;
}
.detailview{
	width: 100%;
	color: #666666;
	justify-content: space-around;
	align-items: center;
}
.incomeview{
	width: 50%;
	height: 100rpx;
	justify-content: space-around;
}
.num{
	font-size: 36rpx;
	font-weight: 500;
	font-family:PingFangSC-Medium;
}
.cashview{
	width: 96%;
	height: 150rpx;
	font-size: 22rpx;
	color: #666666;
	background-color: #FFFFFF; 
	margin: 0 2%;
	justify-content: space-around;
	align-items: center;
}
.moneyview{
	line-height: 50rpx;
}
.incometext{
	font-size: 22rpx;
	font-weight: 400;
	font-family:PingFangSC-Regular;
}
.cash{
	width: 62rpx;
	height: 58rpx;
	margin: auto 7%;
}
.button{
	width: 100rpx;
	height: 45rpx;
	font-size: 22rpx;
	font-family:PingFangSC-Regular;
	font-weight:400;
	color:rgba(26,32,92,1);
	line-height: 45rpx;
	background-color: #F9CE0E;
	border-radius: 20rpx;
	margin-left: 35%;
}

/* bottom */
.bottom{
	width: 96%;
	height: 600rpx;
	margin: auto 2.5%;
	background-color: #FAFAFA;
}
.title{
	width: 97%;
	height: 80rpx;
	align-items: center;
	background-color: #FAFAFA; 
	padding: 0 1.5%;
}
.title2{
	font-size: 30rpx;
	color: #333333;
	font-family:PingFangSC-Regular;
  font-weight:400;
}
.title3{
	text-align: right;
	font-size: 24rpx;
	color: #666666;
	font-family:PingFangSC-Regular;
  font-weight:400;
}
.title view image{
	width: 24rpx;
	height: 12rpx;
	margin-left: 10rpx;
}
.card{
	width: 94%;
	height: 160rpx;
	background-color: #FFFFFF;
	margin: 20rpx auto;
	padding: 25rpx 10rpx;
}
.first{
	width: 100%;
	margin-bottom: 30rpx;
}
.timetext{
	font-size: 30rpx;
	color: #333333;
	font-family:PingFangSC-Regular;
  font-weight:400;
}
.money2{
	text-align: right;
}
.money{
	font-size: 22rpx;
	color: #F85F58;
}
.number{
	font-size: 32rpx;
	color: #F85F58;
	font-family:PingFangSC-Medium;
	font-weight:500;
}
.course{
	width: 99%;
	font-size: 22rpx;
	color: #666666;
	margin-bottom: 15rpx;
}
.type{
	text-align: right;
}
.space{
	width: 50%;
}
.field{
	font-size: 22rpx;
	color: #666666;
}