/* container */
.container{
	height: 1200rpx;
	background-color: #FAFAFA;
	padding-top: 4%;
}

.list{
  width: 100%;
  height: 90rpx;
	font-size: 30rpx;
  color:#666666;
	background-color: #FFFFFF;
  border-bottom:3rpx solid #EEEEEE;
  display:flex;
  flex-direction:row;
	align-items: center;
}
.list1{
  width: 100%;
  height: 120rpx;
	font-size: 30rpx;
  color:#666666;
	background-color: #FFFFFF;
  border-bottom:3rpx solid #EEEEEE;
  display:flex;
  flex-direction:row;
	align-items: center;
}
.text1{
  display:block;
  width: 47%;
  text-align:left;
	margin-left: 3%;
}
.text2{
  display:block;
  width: 47%;
  text-align:right;
	margin-right: 3%;
}
.image{
  width: 90rpx;
  height: 90rpx;
  margin-left: 33%;
	background-color: #F9CE0E;
	border-radius: 100%;
  display:block;
}
.listbtn{
  width:100%;
  height:100rpx;
  margin: 80rpx auto;
  
}
.listbtn button{
  width:80%;
  height: 90rpx;
  background:#F9CE0E;
  color:#292A2E;
  border-radius:50rpx;
}