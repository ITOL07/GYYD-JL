/* container */
.contaoner{
	background-color: #FFFFFF;
}

/* top */
.top{
	width: 100%;
	height: 80rpx;
	line-height: 80rpx;
	border-bottom: 1rpx solid #EEEEEE;
	padding: 0 20rpx;
}
.number{
	color: #666666;
	font-size: 25rpx;
	margin-right: 62%;
}
.confirm{
	color: #333333;
	font-size: 35rpx;
}

/* content */
.content{
	padding: 40rpx 20rpx;
	border-bottom: 1rpx solid #EEEEEE;
}
.content image{
	width: 120rpx;
	height: 120rpx;
}
.second{
	margin-left: 30rpx;
}
.second image{
	margin-left: 20rpx;
}
.name{
	font-size: 35rpx;
	color: #333333;
	align-items: center;
	margin-bottom: 20rpx;
}
.content .seximage{
	width: 30rpx;
	height: 30rpx;
}
.course{
	font-size: 25rpx;
	color: #999999;
}
.button{
	width: 100rpx;
	padding-left: 70rpx;
	padding-top: 45rpx;
}
.button view{
	width: 30rpx;
	height: 30rpx;
	background-color: #EEEEEE;
	border-radius: 100%;
}
.button .selected{
	width: 40rpx;
	height: 40rpx;
}