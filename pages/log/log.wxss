/* container */
.container{
	background-color: #FFFFFF;
}


.demo1{
	width: 345rpx;
  height: 425rpx;
	display:flex;
  flex-direction:column;
	justify-content: space-between;
	position: fixed;
	z-index: 1;
}
.demo2{
	width: 325rpx;
  height: 435rpx;
	background: rgb(0, 0, 0);
	border-radius: 15rpx;
	position: fixed;
	opacity: 0.2;
	z-index: 100;
}




/* inputview */
.inputview{
	width: 100%;
	font-size: 30rpx;
	color: #333333;
	background-color: #FFFFFF;
	margin-bottom: 50rpx;
	padding: 20rpx 5%;
}
.label{
	width: 45%;
	display: block;
}
.input{
	text-align: right;
}

/* listall */
.listall{
  width:100%;
  height: 500rpx;
  display:flex;
  flex-direction:row;
  flex-wrap:wrap;
}
.private_list{
  width: 345rpx;
  height: 425rpx;
	font-size: 22rpx;
	text-align:left;
	border-radius: 10%;
	margin-left: 4%;
	margin-bottom: 70rpx;
	position: relative;
}
.shadow{
	width: 345rpx;
	height: 425rpx;
	background-color: #000000;
	position: relative;
	top: 0;
	opacity: 0.2;
}
.bgcolor{
	background-color: #FFFFFF;
}
.imageview{
	width: 320rpx;
  height: 260rpx;
	margin-bottom: 20rpx;
}
.imageview image{
  display:block;
  width: 320rpx;
  height: 260rpx;
}
.remark{
	width: 50rpx;
	height: 25rpx;
	font-size: 20rpx;
	color: #333333;
	text-align: center;
	line-height: 25rpx;
	background-color: #FBBE44; 
	margin-top: -260rpx;
}
.check{
	width: 30rpx;
	height: 30rpx;
	background: #EEEEEE;
	border-radius: 100%;
	border: 1rpx solid #FFFFFF;
	margin-left: 77%;
	margin-top: 10%;
	position: absolute;
	z-index: 200;
}
.check2{
	width: 35rpx;
	height: 35rpx;
	border-radius: 100%;
	border: 1rpx solid #FFFFFF;
	margin-left: 77%;
	margin-top: 10%;
	position: absolute;
	z-index: 200;
}
.text1{
  font-size: 30rpx;
  color: #666666;
  width:100%; 
}
.text2{
  font-size:25rpx;
  color:#999999;
  width:100%;
}
.buy{
  width:100%;
  color:#F85F58;
}
.price{
	font-size: 30rpx;
}