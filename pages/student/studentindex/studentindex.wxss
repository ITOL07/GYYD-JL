/* container */
.container{
	background-color: #EEEEEE;
}

/* content */
.content{
	width: 100%;
	background-color: #FFFFFF;
}
.viewcontent{
	padding-left: 50rpx;
}
.photo{
	width: 150rpx;
	height: 150rpx;
	border: 7rpx solid #F9CE0E;
	border-radius: 100%;
	margin-top: 25rpx;
}
.viewmsg{
	padding-top: 50rpx;
	margin-left: 40rpx;
	margin-right: 27%;
}
.textname{
	line-height: 70rpx;
	text-align: left;
	align-items: center;
}
.textname2{
	font-size: 35rpx;
	color: #333752;
	margin-right: 15rpx;
}
.sex{
	width: 30rpx;
	height: 30rpx;
}
.space{
	width: 0;
	height: 0;
	border-width: 25rpx 32.5rpx 0;
	border-style: solid;
	border-color:#EC566B transparent transparent;
}
.demo{
	width: 65rpx;
	height: 85rpx;
	text-align: center;
	background-color: #EC566B;
}
.flag{
	font-size: 23rpx;
	color: #FFFFFF;
}
.viewcourse{
	width: 90%;
	height: 150rpx;
	text-align: center;
	padding-bottom: 30rpx;
	margin-top: 50rpx;
	margin-left: 4%;
	justify-content: space-around;
}
.icon{
	width: 70rpx;
	height: 70rpx;
}
.course{
	align-items: center;
	justify-content: space-around;
}

/* message */
.message{
	background-color: #FFFFFF;
	padding: 30rpx 50rpx;
	margin-top: 20rpx;
}
.over{
	height: 200rpx;
	margin-bottom: 30rpx; 
	justify-content: space-around;
}
.textmsg{
	font-size: 35rpx;
	color: #333333;
	font-family:PingFangSC-Regular;
	font-weight:400;
}
.textmsg2{
	margin-right: 70%;
}
.edit{
	width: 25rpx;
	height: 25rpx;
}
.textbirth{
	font-size: 28rpx;
	color: #666666;
}
.bottom{
	width: 100%;
	height: 330rpx;
	justify-content: space-around;
}
.textmsg3{
	margin-right: 60%;
}
.detail{
	font-size: 24rpx;
	color: #999999;
}
.squareview{
	justify-content: space-around;
}
.square{
	width: 200rpx;
	height: 200rpx;
	background-color: #EEEEEE;
}

/* course */
.history{
	width: 100%;
	height: 1200rpx;
	background-color: #FFFFFF;
	padding: 0 60rpx;
}
.schedule{
	margin-bottom: 25rpx;
	align-items: center;
}
.order{
	width: 30rpx;
	height: 30rpx;
	font-size: 40rpx;
	line-height: 25rpx;
	color: #FFFFFF;
	background-color: #F9CE0E;
	margin-left: 70%;
}
.courseschedule{
	width: 77%;
	height: 350rpx;
	border: 1rpx solid #EEEEEE;
	padding: 20rpx 20rpx;
	position: relative;
	justify-content: space-around;
}
.title{
	align-items: center;
}
.sign{
	width: 40rpx;
	height: 40rpx;
	font-size: 30rpx;
	line-height: 15rpx;
	color: #FFFFFF;
	text-align: center;
	background-color: #F9CE0E;
	margin-right: 2%;
}
.textorder{
	font-size: 35rpx;
	color: #EC566B;
	position: absolute;
	right: 3%;
}
.textorder2{
	font-size: 35rpx;
	color: #EC566B;
	position: absolute;
	right: 13%;
}
.coursetime{
	margin-left: 13%;
}
.buttonview{
	width: 100%;
	height: 40rpx;
}
.button{
	width: 100rpx;
	height: 40rpx;
	font-size: 22rpx;
	line-height: 40rpx;
	background-color: #F9CE0E;
	border-radius: 20px;
}
.cancel{
	margin-left: 55%;
	background-color: #EEEEEE;
}
.confirm{
	margin-right: 3%;
}
.courseschedule2{
	width: 77%;
	height: 350rpx;
	border: 1rpx solid #EEEEEE;
	padding: 20rpx 20rpx;
	margin-top: 30rpx;
	margin-bottom: 30rpx;
	justify-content: space-around;
	box-shadow: 0 0 20rpx 5rpx #EEEEEE;
}

/* common */
.font28{
	font-size: 28rpx;
	color: #666666;
}
